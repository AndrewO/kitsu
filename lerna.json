{
  "version": "6.5.1",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "command": {
    "publish": {
      "conventionalCommits": true,
      "changelogPreset": "angular-all-2",
      "message": "chore(release): publish packages",
      "npmClient": "yarn"
    }
  },
  "ignoreChanges": [
    "**/example/**",
    "specification/**",
    "**/*.spec.js",
    "**/*.tgz",
    "**/*.md"
  ]
}
